hello:
	g++ -std=c++11 -Wall -Wextra -g -o hello hello-world.cpp
ackermann:
	g++ -std=c++11 -Wall -Wextra -g -o ackermann ackermann.cpp
berechnung:
	g++ -std=c++11 -Wall -Wextra -g -o berechnung berechnung.cpp
pointertest:
	g++ -std=c++11 -Wall -Wextra -g -o pointertest pointertest.cpp
print:
	g++ -std=c++11 -Wall -Wextra -g -o print print.cpp



clean:
	rm -fv hello 
	rm -fv ackermann
	rm -fv berechnung
	rm -fv pointertest
	rm -fv print

